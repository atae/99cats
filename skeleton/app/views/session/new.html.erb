<h1> Sign In</h1>
<form  action="http://localhost:3000/session" method="post">
  <%= auth_token %>

  <label for="username"> Username</label>
  <input type="text" name="session[user_name]" value="" id = "username">

  <label for="password">Password</label>
  <input type="text" name="session[password]" value="" id = "password">
  <br>
  <br>
  <button style="width:100px;height:30px;">Submit</button>
</form>

<% content_for :footer do %>
  <p>
    Log in here!
  </p>
<% end %>
